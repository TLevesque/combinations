{
  "name": "combinations",
  "displayName": "Combinations",
  "description": "All possibles combinations from series of lists",
  "version": "0.0.2",
  "publisher": "tlevesque2",
  "repository": "https://github.com/TLevesque/combinations.git",
  "engines": {
    "vscode": "^1.27.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.combinations"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.combinations",
        "title": "Combinations: Generate combinations"
      }
    ],
    "keybindings": [
      {
        "command": "extension.combinations",
        "key": "ctrl+alt+o",
        "mac": "ctrl+alt+o",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "eslint": "^4.11.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  }
}
